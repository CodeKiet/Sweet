{
  "name": "sweet-vpn",
  "productName": "SweetVPN",
  "version": "0.1.0",
  "description": "A sweet VPN client",
  "main": "./dist/sweet.js",
  "scripts": {
    "test": "npm test",
    "build": "babel src -d dist --compress && sass --update -f -t compressed src/sass:dist/css",
    "watch": "babel src -D -d dist -w && sass --watch -f src/sass/:dist/css",
    "package": "npm run build && babel-node package",
    "launch": "electron ./dist/menu"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ConnorAtherton/Sweet.git"
  },
  "keywords": [
    "VPN",
    "sweet",
    "electron",
    "react"
  ],
  "author": "Connor Atherton <c.liam.atherton@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ConnorAtherton/Sweet/issues"
  },
  "homepage": "https://github.com/ConnorAtherton/Sweet#readme",
  "dependencies": {
    "electron-rpc": "^1.0.3",
    "menubar": "^2.2.1",
    "react": "^0.13.3"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "dialog": "^0.1.8",
    "electron-packager": "^5.1.0",
    "electron-prebuilt": "^0.33.3",
    "headway": "^0.3.0",
    "sass": "^0.5.0"
  }
}
